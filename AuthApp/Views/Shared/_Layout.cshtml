<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">
    <meta name="description" content="CoreUI - Open Source Bootstrap Admin Template">
    <meta name="author" content="Łukasz Holeczek">
    <meta name="keyword" content="Bootstrap,Admin,Template,Open,Source,jQuery,CSS,HTML,RWD,Dashboard">
    <title>E-Office</title>
    <!-- Icons-->
    <link href="~/Content/vendors/coreui/icons/css/coreui-icons.min.css" rel="stylesheet">
    <link href="~/Content/vendors/flag-icon-css/css/flag-icon.min.css" rel="stylesheet">
    <link href="~/Content/vendors/font-awesome/css/font-awesome.min.css" rel="stylesheet">
    <link href="~/Content/vendors/simple-line-icons/css/simple-line-icons.css" rel="stylesheet">
    <!-- DataTables -->
    <link href="https://coreui.io/demo/vendors/datatables.net-bs4/css/dataTables.bootstrap4.css" rel="stylesheet" />
        
    <!-- Main styles for this application-->
    <link href="~/Content/css/style.css" rel="stylesheet">
    <link href="~/Content/vendors/pace-progress/css/pace.min.css" rel="stylesheet">

</head>
<body class="app header-fixed sidebar-fixed aside-menu-fixed sidebar-lg-show">
    @Html.Partial("_Header")
    <div class="app-body">
        @Html.Partial("_Sidebar")
        <main class="main">
            @RenderBody()
        </main>
        @Html.Partial("_Aside")
    </div>
    @Html.Partial("_Footer")
    <!-- CoreUI and necessary plugins-->
    <script src="~/Content/vendors/jquery/js/jquery.min.js"></script>
    <script src="~/Content/vendors/popper.js/js/popper.min.js"></script>
    <script src="~/Content/vendors/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/Content/vendors/pace-progress/js/pace.min.js"></script>
    <script src="~/Content/vendors/perfect-scrollbar/js/perfect-scrollbar.min.js"></script>
    <script src="~/Content/vendors/coreui/coreui/js/coreui.min.js"></script>
    <!-- DATA TABES SCRIPT -->
    <script src="~/Content/vendors/datatables/jquery.dataTables.js" type="text/javascript"></script>
    <script src="~/Content/vendors/datatables/dataTables.bootstrap.js" type="text/javascript"></script>
    <!-- Plugins and scripts required by this view-->
    <script src="~/Content/vendors/chart.js/js/Chart.min.js"></script>
    <script src="~/Content/vendors/coreui/coreui-plugin-chartjs-custom-tooltips/js/custom-tooltips.min.js"></script>
    <script src="~/Content/js/main.js"></script>
    <script type="text/javascript">
        $(function () {
            $("#dataTables").dataTable();
        });
    </script>
</body>
</html>
